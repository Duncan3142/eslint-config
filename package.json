{
	"name": "@duncan3142/eslint-config",
	"description": "ESLint configuration",
	"keywords": [],
	"version": "1.0.0",
	"author": "Duncan Giles",
	"type": "module",
	"license": "MIT",
	"engines": {
		"node": "^18.0.0",
		"pnpm": "^8.0.0"
	},
	"exports": {
		".": {
			"types": "./.tsc/dist/eslint.config.d.cts",
			"require": "./.tsc/dist/eslint.config.cjs"
		}
	},
	"files": [
		"./.tsc/dist/*.cjs",
		"./.tsc/dist/*.map",
		"./.tsc/dist/*.cts"
	],
	"peerDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"eslint": "^8.12.0",
		"eslint-import-resolver-typescript": "^3.2.5",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-vitest": "^0.1.4"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^5.59.5",
		"@typescript-eslint/parser": "^5.59.5",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-vitest": "^0.1.5"
	},
	"scripts": {
		"prepare": "husky install",
		"clean": "rm -rf .prettier .package .tsc",
		"format:prettier": "prettier --check --cache --cache-location='.prettier/cache' --cache-strategy=content .",
		"format": "turbo run format:all --cache-dir=\"./.turbo\"",
		"assets:tsc": "tsc --build",
		"assets": "turbo run assets:all --cache-dir=\"./.turbo\"",
		"build": "turbo run build:all --cache-dir=\"./.turbo\""
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.1",
		"@commitlint/config-conventional": "^17.6.3",
		"@duncan3142/prettier-cnfg": "^1.0.0",
		"@duncan3142/tsc-cnfg": "^1.0.2",
		"@types/eslint": "^8.37.0",
		"commitlint": "^17.6.3",
		"husky": "^8.0.3",
		"turbo": "^1.9.3"
	}
}
